    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "keywords": [
    "memoization",
    "finalization",
    "registry",
    "memory",
    "leak"
  ],
  "dependencies": {
    "mild-map": "^1.1.0"
  },
  "devDependencies": {
    "fava": "^0.1.2",
    "tsex": "^2.1.0",
    "typescript": "^4.9.5"
  }
